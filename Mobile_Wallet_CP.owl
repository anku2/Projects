    <!-- http://www.semanticweb.org/omsairam/ontologies/2018/10/mobile_wallets.owl#PeriodicStatement -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/omsairam/ontologies/2018/10/mobile_wallets.owl#PeriodicStatement">
        <rdf:type rdf:resource="http://www.semanticweb.org/omsairam/ontologies/2018/10/mobile_wallets.owl#TILA_Titles"/>
