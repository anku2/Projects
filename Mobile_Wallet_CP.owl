    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/omsairam/ontologies/2018/10/mobile_wallets.owl#Advertising -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/omsairam/ontologies/2018/10/mobile_wallets.owl#Advertising">
