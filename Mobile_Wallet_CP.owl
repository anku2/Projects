    <!-- http://www.semanticweb.org/omsairam/ontologies/2018/10/mobile_wallets.owl#PeriodicRate -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/omsairam/ontologies/2018/10/mobile_wallets.owl#PeriodicRate">
        <rdf:type rdf:resource="http://www.semanticweb.org/omsairam/ontologies/2018/10/mobile_wallets.owl#TILA_Definitions"/>
